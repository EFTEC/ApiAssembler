  "type": "library",
  "require": {
    "php": ">=7.2.5",
    "eftec/clione": "^1.21",
    "eftec/pdoone": "^3.9",
    "eftec/validationone": "^2.4",
    "eftec/routeone": "^1.24",
    "ext-json": "*",
    "eftec/cacheone": "^2.12.4",
    "eftec/authone": "^1.4"
